{
  "https_port": 443,
  "Version": {
    "Major": 1,
    "Minor": 0,
    "Build": 1
  },
  "Admin": {
    "Email": "",
    "Password": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "SecurityKey": "identity-security",
    "Issuer": "identity",
    "Audience": "audience",
    "Lifetime": "00:00:30"
  },
  "Google": {
    "ClientId": "ID.apps.googleusercontent.com",
    "ClientSecret": "SECRET"
  },
  "Mongo": {
    "ConnectionString": "mongodb://localhost:27017/",
    "DatabaseName": "identity"
  },
  "Redis": {
    "ConnectionString": "redis://localhost:6379"
  },
  "Email": {
    "From": "info@mail.com",
    "FromName": "ProjectName",
    "SmtpUsername": "USERNAME",
    "SmtpPassword": "PASSWORD",
    "Host": "email-smtp.eu-central-1.amazonaws.com",
    "ConfigSet": null,
    "Port": 587,
    "EnableSsl": true
  },
  "Password": {
    "RequiredLength": 6,
    "RequiredUniqueChars": 0,
    "RequireNonAlphanumeric": true,
    "RequireLowercase": true,
    "RequireUppercase": true,
    "RequireDigit": true
  },
  "InvalidPassword": {
    "InvalidCountToBlock": 5,
    "BlockTime": "00:02:00"
  },
  "AllowedHosts": "*",
  "IdentityScope": {
    "Email":  ["link", "profile"],
    "Google": ["link", "profile"],
    "Guest": []
  }
}
